<p id="notice"><%= notice %></p>

<p>
  <strong>Title:</strong>
  <%= @article.title %>
</p>

<p>
  <strong>Body:</strong>
  <%= @article.body %>
</p>

<div>
  <% @article.photos.each do |photo| %>
    <%= cl_image_tag photo.key %>
    <div>
      <%= cl_image_path photo.key %>
    </div>
  <% end %>
</div>

<%= link_to 'Edit', edit_article_path(@article) %> |
<%= link_to 'Back', articles_path %>
